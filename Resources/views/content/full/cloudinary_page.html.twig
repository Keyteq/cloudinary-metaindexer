<form method="get">
    <input type="text" name="s" value="{{ app.request.get('s') }}" />
    <button type="submit">{{ 'keyteq.cloudinary_meta_indexer.search'|trans }}</button>
</form>

<div class="row">
    {% for resource in resources %}
        <div class="col-xs-6 col-md-4 col-lg-3">
            {% include 'KeyteqCloudinaryMetaIndexerBundle:cloudinary:resource.html.twig' with {'resource': resource} %}
        </div>
    {% endfor %}
</div>
